    "activeTab",
    "scripting",
    "notifications"
  ],
  "action": {
    "default_icon": {
      "128": "assets/bitcoin.png"
    },
    "default_title": "KÃ©tautomatique",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.binancefuture.com/*", "https://*.binance.com/*"],
      "css": ["styles/btnStyle.css"],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "128": "assets/bitcoin.png"
  }
}